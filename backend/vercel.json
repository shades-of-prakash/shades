{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/products/(.*)",
			"dest": "/server.js"
		},
		{
			"src": "/api/user/(.*)",
			"dest": "/server.js"
		},
		{
			"src": "/(.*)",
			"dest": "/server.js"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'none'; script-src 'self' 'unsafe-inline' 'unsafe-eval' blob:; object-src 'none'; style-src 'self' 'unsafe-inline';"
				}
			]
		}
	]
}
